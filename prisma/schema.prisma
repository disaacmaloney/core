generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model USE_USERS {
  USE_ID Int @id @default(autoincrement())
  USE_USERNAME String @unique
  USE_PASSWORD String
  USE_NAME String
  USE_LASTNAME String
}